    "globals": "^15.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.709.0",
    "@tailwindcss/container-queries": "^0.1.1",
